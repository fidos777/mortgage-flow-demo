{
  "name": "mortgage-flow-demo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "i18n:check": "npx tsx scripts/check-i18n.ts",
    "demo:health": "bash scripts/demo-health.sh",
    "demo:prep": "p=$(lsof -ti tcp:3000 2>/dev/null) && [ -n \"$p\" ] && kill -9 $p || true; npm run build",
    "demo:start": "npm run start",
    "demo:full": "npm run demo:prep && npm run demo:start"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.93.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.34.0",
    "lucide-react": "^0.263.1",
    "next": "14.2.29",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3"
  }
